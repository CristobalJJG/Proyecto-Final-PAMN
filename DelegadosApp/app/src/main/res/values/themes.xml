<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.DelegadosApp" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue</item>
        <item name="colorPrimaryVariant">@color/light_blue</item>
        <item name="colorOnPrimary">@color/dark_blue</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/yellow</item>
        <item name="colorSecondaryVariant">@color/light_yellow</item>
        <item name="colorOnSecondary">@color/dark_yellow</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/dark_white</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.DelegadosApp.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.DelegadosApp.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.DelegadosApp.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>